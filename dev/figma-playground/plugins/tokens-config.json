      "options": {
        "start": {
          "figma": "min",
          "class": "f-a-start"
        },
        "center": {
          "figma": "center",
          "class": "f-a-center"
        },
        "end": {
          "figma": "max",
          "class": "f-a-end"
        },
        "baseline": {
          "figma": "baseline",
          "class": "f-a-baseline"
        },
        "stretch": {
          "figma": "fill",
          "class": "f-a-stretch"
        }
