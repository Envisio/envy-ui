{
  "containerManifest": {
    "display": {
      "options": {
        "flex": {
          "figma": "auto-layout-horizontal",
          "class": "f"
        },
        "flex-inline": {
          "figma": "auto-layout-horizontal",
          "class": "f-inline"
        },
        "block": {
          "figma": "fixed",
          "class": "block"
        },
        "grid": {
          "figma": "auto-layout-grid",
          "class": "g"
        }
      }
    },
    "direction": {
