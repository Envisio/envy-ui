        "column": {
          "figma": "vertical",
          "class": "f-column"
        },
        "row-reverse": {
          "figma": "horizontal-reverse",
          "class": "f-row-reverse"
        },
        "column-reverse": {
          "figma": "vertical-reverse",
          "class": "f-column-reverse"
        }
      }
    },
    "gap": {
      "options": {
        "0": {
