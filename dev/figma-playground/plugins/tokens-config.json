        },
        "2": {
          "figma": 2,
          "class": "f-gap-2"
        },
        "3": {
          "figma": 3,
          "class": "f-gap-3"
        },
        "4": {
          "figma": 4,
          "class": "f-gap-4"
        },
        "5": {
          "figma": 5,
          "class": "f-gap-5"
        },
        "10": {
          "figma": 10,
          "class": "f-gap"
        },
        "15": {
          "figma": 15,
          "class": "f-gap-15"
        },
        "20": {
          "figma": 20,
          "class": "f-gap-20"
        },
        "25": {
          "figma": 25,
          "class": "f-gap-25"
        },
        "30": {
          "figma": 30,
          "class": "f-gap-30"
        }
      }
